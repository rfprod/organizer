{
  "name": "organizer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze:app": "npm run build:app:stats; npx webpack-bundle-analyzer ./dist/organizer/stats.json",
    "build": "ng build",
    "build:app:prod": "ng build --project organizer --prod",
    "build:app:stats": "ng build --stats-json --project organizer --configuration analyze-bundle",
    "build:electron": "electron-builder",
    "build:electron:all": "electron-builder -mwl",
    "build:electron:mac": "electron-builder --macos",
    "build:electron:nix": "electron-builder --linux deb tar.xz",
    "build:electron:win": "electron-builder --windows",
    "compodoc:generate": "compodoc -p src/tsconfig.app.json",
    "compodoc:generate:report": "compodoc -p src/tsconfig.app.json && cp -r ./documentation ./dist/documentation",
    "compodoc:serve": "compodoc -p src/tsconfig.app.json -s",
    "e2e": "ng e2e",
    "e2e:headless": "ng e2e --headless --watch=false",
    "postinstall": "npx sort-package-json && npx electron-builder install-app-deps && npm run ngcc",
    "install:global": "bash shell/install.sh global",
    "install:project": "bash shell/install.sh project",
    "lint": "ng run organizer:lint && ng run organizer:e2e-lint",
    "lint:html": "npx prettier -c src/**/*.html",
    "lint:html:fix": "npx prettier -c --write src/**/*.html",
    "lint:precommit": "bash shell/precommit.sh lint",
    "ng": "ng",
    "ngcc": "ngcc --tsconfig tsconfig.base.json",
    "ngcc:ci": "ngcc --tsconfig tsconfig.base.json --async false",
    "start": "concurrently \"npm:start:server\" \"npm:start:client\"",
    "start:client": "ng serve",
    "start:electron": "electron .",
    "start:server": "node server.js",
    "stylelint": "npx stylelint \"./src/**/*.scss\"",
    "stylelint:fix": "npx stylelint \"./src/**/*.scss\" --fix",
    "test": "ng test organizer --watch=false --coverage=false",
    "test:report": "ng test organizer --watch=false",
    "test:report:dist": "ng test organizer --watch=false && cp -r ./coverage ./dist/coverage",
    "test:watch": "ng test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@angular/animations": "10.1.2",
    "@angular/cdk": "10.2.1",
    "@angular/common": "10.1.2",
    "@angular/compiler": "10.1.2",
    "@angular/core": "10.1.2",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.1.2",
    "@angular/material": "10.2.1",
    "@angular/material-moment-adapter": "10.2.1",
    "@angular/platform-browser": "10.1.2",
    "@angular/platform-browser-dynamic": "10.1.2",
    "@angular/router": "10.1.2",
    "@ngneat/until-destroy": "8.0.2",
    "@ngxs/form-plugin": "3.7.0",
    "@ngxs/logger-plugin": "3.7.0",
    "@ngxs/router-plugin": "3.7.0",
    "@ngxs/store": "3.7.0",
    "body-parser": "1.19.0",
    "components-font-awesome": "5.9.0",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "d3": "5.16.0",
    "dotenv": "8.2.0",
    "electron-squirrel-startup": "1.0.0",
    "express": "4.17.1",
