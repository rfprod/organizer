{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [ "jsdoc", "closure" ]
	},
	"opts": {
		"destination": "./logs/jsdoc/build"
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"path": "unk-docstrap",
		"theme": "flatly",
		"linenums": true,
		"systemName": "Ng2NS Build",
		"dateFormat": "MMMM DD YYYY, h:mm:ss a",
		"footer": "",
		"copyright": "",
		"includeDate": true,
		"navType": "vertical",
		"collapseSymbols": true,
		"inverseNav": false,
		"outputSourceFiles": true,
		"outputSourcePath": true,
		"syntaxTheme": "default",
		"sort": true,
		"search": true,
		"cleverLinks": false,
		"momospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	}
}
